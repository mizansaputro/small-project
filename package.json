{
    "name": "rey-task-web",
    "version": "0.0.1",
    "private": true,
    "scripts": {
        "dev": "next dev",
        "build": "next build",
        "start": "next start",
        "svgr": "npx @svgr/cli -d src/components/icons --ignore-existing --icon --typescript public/icons",
        "svgr-replace": "npx @svgr/cli -d src/components/icons --icon --typescript public/icons",
        "build:analyze": "ANALYZE=true next build",
        "lint": "eslint '**/*.{js,jsx,ts,tsx}'",
        "prettier": "prettier --write '**/*.{js,jsx,ts,tsx}'",
        "test": "jest -c jest.config.js",
        "test-coverage": "jest --collectCoverage=true",
        "prepare": "husky install",
        "pre-commit": "lint-staged",
        "todo-server": "json-server --watch ./src/datastore/json/todo.json --port 3004",
        "invoice-server": "json-server --watch ./src/datastore/json/invoice.json --port 3005"
    },
    "dependencies": {
        "@date-io/date-fns": "2.16.0",
        "@emotion/cache": "11.10.5",
        "@emotion/css": "11.10.5",
        "@emotion/react": "11.10.5",
        "@emotion/server": "11.10.0",
        "@emotion/styled": "11.10.5",
        "@material-ui/pickers": "^3.3.10",
        "@mui/icons-material": "5.11.0",
        "@mui/lab": "5.0.0-alpha.118",
        "@mui/material": "5.11.7",
        "@mui/styles": "5.11.7",
        "@mui/x-date-pickers": "^5.0.16",
        "@next/bundle-analyzer": "12.3.4",
        "axios": "1.3.0",
        "date-fns": "2.29.3",
        "dayjs": "^1.11.7",
        "moment": "^2.29.4",
        "next": "12.3.4",
        "next-compose-plugins": "2.2.1",
        "next-translate": "1.6.0",
        "prettier": "2.8.3",
        "react": "18.2.0",
        "react-datepicker": "^4.10.0",
        "react-dom": "18.2.0",
        "react-use": "17.4.0",
        "sharp": "0.31.3",
        "yup": "0.32.11",
        "zustand": "4.3.2"
    },
    "devDependencies": {
        "@commitlint/cli": "17.4.2",
        "@commitlint/config-conventional": "17.4.2",
        "@emotion/eslint-plugin": "11.10.0",
        "@next/eslint-plugin-next": "12.3.4",
        "@svgr/cli": "6.5.1",
        "@testing-library/jest-dom": "^5.16.5",
        "@testing-library/react": "^13.4.0",
        "@testing-library/user-event": "14.4.3",
        "@types/jest": "29.4.0",
        "@types/node": "18.11.18",
        "@types/react": "18.0.27",
        "@types/react-dom": "18.0.10",
        "@typescript-eslint/eslint-plugin": "5.50.0",
        "@typescript-eslint/parser": "5.50.0",
        "commitizen": "4.3.0",
        "css-loader": "6.7.3",
        "cz-conventional-changelog": "3.3.0",
        "dotenv": "16.0.3",
        "eslint": "8.33.0",
        "eslint-config-next": "12.3.4",
        "eslint-config-prettier": "8.6.0",
        "eslint-plugin-prettier": "4.2.1",
        "husky": "8.0.3",
        "identity-obj-proxy": "3.0.0",
        "jest": "^29.4.2",
        "jest-environment-jsdom": "29.4.1",
        "lint-staged": "13.1.0",
        "nock": "13.3.0",
        "start-server-and-test": "1.15.3",
        "style-loader": "3.3.1",
        "ts-jest": "29.0.5",
        "typescript": "4.9.5"
    },
    "lint-staged": {
        "*.{js,jsx,ts,tsx}": [
            "yarn lint -- --quiet --fix",
            "yarn prettier"
        ]
    },
    "config": {
        "commitizen": {
            "path": "./node_modules/cz-conventional-changelog"
        }
    }
}
